#!/bin/csh

setenv MDDRIVER_DIR "@CMAKE_INSTALL_PREFIX@"
setenv MDDRIVER_PATH "${MDDRIVER_DIR}/@BIN_INSTALL_DIR@"
setenv MDDRIVER_LIBRARY_DIR "${MDDRIVER_DIR}/@LIB_INSTALL_DIR@"
setenv MDDRIVER_INCLUDE_DIR "${MDDRIVER_DIR}/@INCLUDE_INSTALL_DIR@"

if (${?PATH} == 0)  setenv PATH ""
 setenv PATH "${PATH}:${MDDRIVER_PATH}"
if (${?LD_LIBRARY_PATH} == 0) setenv LD_LIBRARY_PATH ""
 setenv LD_LIBRARY_PATH "${LD_LIBRARY_PATH}:${MDDRIVER_LIBRARY_DIR}"
if (${?DYLD_LIBRARY_PATH} == 0) setenv DYLD_LIBRARY_PATH ""
 setenv DYLD_LIBRARY_PATH "${DYLD_LIBRARY_PATH}:${MDDRIVER_LIBRARY_DIR}"
